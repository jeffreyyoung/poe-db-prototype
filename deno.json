{
  "tasks": {
    "dev": "deno test --watch --ignore=replicache_e2e_test.ts",
    "test": "deno test --ignore=replicache_e2e_test.ts",
    "test:full": "deno test --allow-net --allow-env",
    "docker": "docker-compose up --build",
    "test:docker": "REPLICACHE_BASE_URL=http://localhost:8000 deno test --allow-net --allow-env",
    "backend": "deno run --allow-net --allow-read --allow-write backend/replicache_server.ts"
  },
  "imports": {
    "@std/assert": "jsr:@std/assert@1",
    "@std/testing": "jsr:@std/testing@^1.0.11"
  },
  "lint": {
    "rules": {
      "require-await": "off",
      "no-explicit-any": "off"
    }
  }
}
